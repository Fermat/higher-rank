u :: forall x . F x
u = u 

v :: forall x . F (F x) 
v = u

data N :: * where

data S :: * -> * where

data Vec :: * -> * -> * where
  VNil :: forall a . Vec a N
  VCons :: forall a n . a -> Vec a n -> Vec a (S n)

primitive undefined :: forall a . a
shead :: forall a n . Vec a (S n) -> a
shead (VCons x xs) = x


