primitive f :: forall x a . E a x -> forall x . (E a x -> E a x)
primitive a :: E Emp Emp

-- eta :: forall a x . (forall x . E a x -> E a x) -> E a x -> E a x
test1 :: E Emp Emp
test1 = let 
            eta  = \ u a -> u a in
        eta (f a) a

primitive f1 :: forall x a . E a x -> forall x . (E a x)

test2 :: E Emp Emp
test2 = let e :: forall x . E Emp x
            e = f1 a in e

test3 :: forall x . F x
test3 = fix l''

